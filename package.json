  "private": true,
  "engines": {
    "node": ">=14.17.3 <19.0.0",
    "yarn": ">=1.22.5 <2.0.0"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "echo \"no test specified\" && exit 0",
    "lint": "yarn lint:type && yarn lint:eslint && yarn lint:prettier",
    "lint:type": "tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.5",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
