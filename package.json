  "private": true,
  "engines": {
    "node": ">=14.17.3 <15.0.0",
    "yarn": ">=1.22.5 <2.0.0"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "echo \"no test specified\" && exit 0",
    "lint": "yarn lint:type && yarn lint:eslint && yarn lint:prettier",
    "lint:type": "tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
